      "Enable": true,
      "Kind": "space",
      "PipelineIndentation": "IncreaseIndentationAfterEveryPipeline"
    },
    "PSUseConsistentWhitespace": {
      "Enable": true,
      "CheckInnerBrace": true,
      "CheckOpenBrace": true,
      "CheckOpenParen": true,
