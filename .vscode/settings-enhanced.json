      ],
      "icon": "terminal-powershell",
      "color": "terminal.ansiBlue"
    },
    "PowerShell Debug Mode": {
      "path": "pwsh.exe",
      "args": [
        "-NoProfile",
        "-NoExit",
        "-Command",
        "$env:BUSBUDDY_DEBUG_ENABLED='true'; $env:BUSBUDDY_VERBOSE_LOGGING='true'; . '${workspaceFolder}\\load-bus-buddy-profiles.ps1' -Force; Write-Host 'üêõ Debug Mode Enabled' -ForegroundColor Red"
      ],
      "icon": "debug",
      "color": "terminal.ansiRed"
    },
    "PowerShell Admin Enhanced": {
      "path": "pwsh.exe",
      "args": [
        "-NoProfile",
        "-NoExit",
        "-Command",
        "Start-Process pwsh.exe -ArgumentList '-NoProfile', '-NoExit', '-Command', \"`. '${workspaceFolder}\\load-bus-buddy-profiles.ps1' -Force; Write-Host 'Administrator PowerShell Ready' -ForegroundColor Yellow\"\" -Verb RunAs"
      ],
      "icon": "shield",
      "color": "terminal.ansiYellow"
    }
  },
